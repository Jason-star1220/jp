#include"stdio.h"

int main()
{
	char str[8][100]={'\0'};
	FILE *fp=fopen("D:\\np.txt","r");
	if(fp==NULL)
	{
		printf("Error\n");
		return 0;
	}
	int i=0;
	while(fgets(str[i],100,fp)!=NULL)
	{	
		i++;
	}
	
	fclose(fp);
	
	FILE *pfp=fopen("D:\\new1.txt","w");	
	fputs(str[0],pfp);
	fclose(pfp);
	FILE *ofp=fopen("D:\\new2.txt","w");
	
	fputs(str[7],ofp);


}
